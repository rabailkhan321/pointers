/*a program to swap tow integers (call by reference)*/

#include<stdio.h>
void swap(int *x,int *y);
int main()
{
    int a=3, b=4;
    printf("the value of a before swap is %d and b is %d",a,b);
    swap(&a,&b);
    printf("the value of a after swap is %d and b is %d",a,b);
}
swap(int *x,int *y)
{
    int temp;
    temp= *x;
    *x=*y;
    *y=temp;
    
}
